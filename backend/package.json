{
  "name": "jobs-analytics-backend",
  "version": "2.0.0",
  "description": "Backend API for UN Jobs Analytics Dashboard with PostgreSQL + SQLite caching",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "migrate": "node dist/scripts/migrate.js",
    "seed": "node dist/scripts/seed.js",
    "classify-all": "node dist/scripts/classifyExisting.js",
    "sync": "node dist/scripts/syncFromPostgres.js",
    "sync:dev": "npx ts-node src/scripts/syncFromPostgres.ts",
    "push:neon": "npx ts-node src/scripts/pushToNeon.ts"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "better-sqlite3": "^9.6.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/better-sqlite3": "^7.6.13",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.7",
    "@types/uuid": "^9.0.7",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "keywords": [
    "jobs",
    "analytics",
    "postgresql",
    "sqlite",
    "classification",
    "api"
  ],
  "author": "Jobs Analytics Team",
  "license": "MIT"
}
