{
  "functions": {
    "api/*.ts": {
      "runtime": "@vercel/node@3.x"
    }
  },
  "rewrites": [
    { "source": "/api/dashboard/sync-status", "destination": "/api/sync-status" },
    { "source": "/api/dashboard/jobs", "destination": "/api/jobs" },
    { "source": "/api/dashboard/all", "destination": "/api/all" },
    { "source": "/api/dashboard/sync", "destination": "/api/sync-status" },
    { "source": "/api/test", "destination": "/api/test" }
  ]
}
